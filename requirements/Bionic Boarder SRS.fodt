     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_kj9rygdikkwy" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Standards Compliance</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_i0u468c6zuoi" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Hardware Limitations</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P33" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_s5z7etvnezm" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Attributes</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_wtdgs76z3rz6" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Availability</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_bjitcmwhey5x" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Security</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_e5po9qanxzue" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Maintainability</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_rj4tlkisvdbj" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Transferability/Conversion</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P33" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_z8c4ju4ajiuv" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Other Requirements</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_uoif9fuqhbu1" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Operations</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_ume1k7oyry7d" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Site Adaptation</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P32" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_wkds14o9trm0" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Appendixes</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P33" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_clp35ii7xzat" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:soft-page-break/><text:span text:style-name="T24">Annex 1: Characteristics of a good SRS</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P33" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_l587f99ytp53" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Annex 2: Organizing the specific requirements</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P33" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_lg3ompi10x86" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Annex 3: SRS templates</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_fq84tbgccxb3" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Template of SRS Section 3 organized by feature</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_vpigomc8pdzb" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Template of SRS Section 3 organized by user class</text:span></text:a><text:span text:style-name="T24"/></text:p>
     <text:p text:style-name="P34" loext:marker-style-name="T24"><text:a xlink:type="simple" xlink:href="#_8avjwjy42bpe" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T24">Template of SRS Section 3 showing multiple organizations</text:span></text:a></text:p>
